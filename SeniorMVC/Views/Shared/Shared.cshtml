@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title></title>
    <meta http-equiv="Content-Type" content="text/html; charset=windows-1252" />
    <title>SimplaAdmin</title>

    <!--                       CSS                       -->
    <!-- Reset Stylesheet -->
    <link rel="stylesheet" href=@Url.Content("SimplaAdmin_Dash_files/reset.css") type="text/css" media="screen">

    <!-- Main Stylesheet -->
    <link rel="stylesheet" href=@Url.Content("SimplaAdmin_Dash_files/style.css") type="text/css" media="screen">

    <!-- Invalid Stylesheet. This makes stuff look pretty. Remove it if you want the CSS completely valid -->
    @*<link rel="stylesheet" href=@Url.Content("SimplaAdmin_Dash_files/invalid.css") type="text/css" media="screen">*@

    <!-- Colour Schemes

        Default colour scheme is green. Uncomment prefered stylesheet to use it.
    -->
    <link rel="stylesheet" href="resources/css/blue.css" type="text/css" media="screen" />
    <!--
        <link rel="stylesheet" href="resources/css/red.css" type="text/css" media="screen" />

        -->
    <!-- Internet Explorer Fixes Stylesheet -->
    <!--[if lte IE 7]>
        <link rel="stylesheet" href="resources/css/ie.css" type="text/css" media="screen" />
    <![endif]-->
    <!--                       Javascripts                       -->
    <!-- jQuery -->
    <script type="text/javascript" src=@Url.Content("/SimplaAdmin_Dash_files/jquery-1.3.2.min.js.download")></script>

    <!-- jQuery Configuration -->
    <script type="text/javascript" src=@Url.Content("SimplaAdmin_Dash_files/simpla.jquery.configuration.js.download")></script>

    <!-- Facebox jQuery Plugin -->
    <script type="text/javascript" src=@Url.Content("SimplaAdmin_Dash_files/facebox.js.download")></script>

    <!-- jQuery WYSIWYG Plugin -->
    <script type="text/javascript" src=@Url.Content("SimplaAdmin_Dash_files/jquery.wysiwyg.js.download")></script>

    <!-- jQuery Datepicker Plugin -->
    <script type="text/javascript" src=@Url.Content("SimplaAdmin_Dash_files/jquery.datePicker.js.download")></script>
    <script type="text/javascript" src=@Url.Content("SimplaAdmin_Dash_files/jquery.date.js.download")></script>
    <!--[if IE]><script type="text/javascript" src="resources/scripts/jquery.bgiframe.js"></script><![endif]-->
    <!-- Internet Explorer .png-fix -->
    <!--[if IE 6]>
        <script type="text/javascript" src="resources/scripts/DD_belatedPNG_0.0.7a.js"></script>
        <script type="text/javascript">
            DD_belatedPNG.fix('.png_bg, img, li');
        </script>
    <![endif]-->

</head>
<body>
    @RenderBody();
</body>
</html>
